<h1>Cadastrar Bibliotec√°rio</h1>

<%= form_with(model: @bibliotecario) do |form| %>
        
        <div>
            <%= form.label :nome %>
            <%= form.text_field :nome %>
        </div>

        
        <div>
          <%= form.label :email %>
          <%= form.email_field :email %>
        </div>

        <div>
          <%= form.label :password %>
          <%= form.password_field :password %>
        </div>

        <div>
          <%= form.label :password_confirmation %>
          <%= form.password_field :password_confirmation %>
        </div>

        <div>
          <%= form.submit %>
        </div>
<% end %>